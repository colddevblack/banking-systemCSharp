@model BancoExt.Models.TipoContaModel

@{
    ViewBag.Title = "Cadastro de Tipo de Contas";
}

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h2>Cadastro de Tipo de Contas</h2>

                    <h2>Novo Cadastro -  Tipo de Contas</h2>



                    @using (Html.BeginForm("CadastrarTipoConta", "TipoConta"))

                    {
                        <div class="row">
                            <div class="col-lg-4">
                                @Html.LabelFor(obj => obj.nomeconta)
                                @Html.TextBoxFor(obj => obj.nomeconta, new { @class = "form-control" })
                            </div>
                        </div>



                        <div>
                            <input type="submit" value="Salvar" class="btn btn-warning" />

                        </div>
                    }
             </div>
         </div>
       </div>
     </div>
</div>